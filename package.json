  "name": "PoppinProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios:simulator": "concurrently \"npm start\" \"open -a Simulator && react-native run-ios --simulator='PickplySimulator'\"",
    "ios:device": "concurrently \"npm start\" \"react-native run-ios --device 'puretension'\"",
    "android:device": "concurrently \"npm start\" \"react-native run-android\"",
    "dev:ios:simulator": "concurrently \"npm start\" \"open -a Simulator && react-native run-ios --simulator='PickplySimulator' --scheme Develop --configuration Debug\"",
    "dev:ios:device": "concurrently \"npm start\" \"react-native run-ios --device 'puretension' --scheme Develop --configuration Debug\"",
    "dev:android": "concurrently \"npm start\" \"react-native run-android --mode=developDebug\"",
    "stg:ios:simulator": "concurrently \"npm start\" \"open -a Simulator && react-native run-ios --simulator='PickplySimulator' --scheme Stage --configuration Debug\"",
    "stg:ios:device": "concurrently \"npm start\" \"react-native run-ios --device 'puretension' --scheme Stage --configuration Debug\"",
    "stg:android": "concurrently \"npm start\" \"react-native run-android --mode=stagedebug\"",
    "prd:ios:simulator": "concurrently \"npm start\" \"open -a Simulator && react-native run-ios --simulator='PickplySimulator' --scheme Production --configuration Release\"",
    "prd:ios:device": "concurrently \"npm start\" \"react-native run-ios --device 'puretension' --scheme Production --configuration Release\"",
    "prd:android": "concurrently \"npm start\" \"react-native run-android --mode=productrelease\""
  },
